<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycheckboxadapter">

    <application
        android:allowBackup="true"
        android:icon="@drawable/shopping_list"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shopping_list"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCheckBoxAdapter">
        <activity android:name=".ListViewActivity"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyCheckBoxAdapter.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>